{
  "name": "t-fileheader",
  "displayName": "t-fileheader",
  "description": "file header inserter",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "t-fileheader",
        "title": "Fileheader inserter"
      }
    ],
    "keybindings": [
      {
        "command": "t-fileheader",
        "key": "ctrl+alt+h",
        "when": "editorTextFocus",
        "mac": "cmd+alt+h"
      }
    ],
    "configuration": {
      "title": "File Header",
      "properties": {
        "fileHeader.authorName": {
          "type": "string",
          "default": "Your Name",
          "description": "Author name for the file header."
        },
        "fileHeader.authorEmail": {
          "type": "string",
          "default": "Your Email",
          "description": "Author email for the file header."
        },
        "fileHeader.description": {
          "type": "string",
          "default": "Brief description here",
          "description": "Description to be included in the file header."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "build": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.88.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "vsce": "^2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@192.168.123.37:10022/yikim/vscode-fileheader.git"
  }
}
